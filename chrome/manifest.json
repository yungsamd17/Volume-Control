{
  "manifest_version": 2,
  "author": "Sam @yungsamd17",
  "name": "Volume Control",
  "version": "1.1",
  "description": "Adjust volume up to 200%",
  
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
	"permissions": [
    "activeTab",
    "storage",
    "tabCapture"
  ],
	"background": {
		"scripts": [
      "background.js"
    ]
  },
	"browser_action": {
		"default_popup": "popup/index.html"
  },
  "options_ui": {
    "page": "options/index.html",
    "browser_style": true,
    "chrome_style": true
  }
}